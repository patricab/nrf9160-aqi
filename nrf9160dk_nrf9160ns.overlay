&uart0 {
	status = "okay";
	current-speed = <9600>;
	tx-pin = <0>;
	rx-pin = <1>;
};

&i2c2 {
	status = "okay";
	compatible = "nordic,nrf-twim";
	sda-pin = <30>;
	scl-pin = <31>;
    clock-frequency = <I2C_BITRATE_STANDARD>;
/*
	sps30@69 {
		compatible = "sensirion,sps30";
		label = "SPS30";
		reg = <0x69>;
	};
*/
};

&spi3 {
    status = "disabled";
};
